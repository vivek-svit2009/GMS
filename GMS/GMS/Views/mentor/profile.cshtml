                <input type="button" name="cancel" value="Cancel" class="btn btn-default edit_anchor">
            </div>

        </div>


    </div>


</div>
<script>
        $("#eduDiv").on('change', '.industry_roles select', function () {
            var val = $(this).val();
            var text_div = $(this).parents('.contri');
            if (val == 'Others') {
                text_div.find('.add_other_input').html('<input type="text" class="form-control other_input_box" style="margin-top: 20px;" placeholder="Other Roles" name="other_industry_roles[]"/>');
            }
            else if (val !== 'Others') {
                text_div.find('.add_other_input').html('');
                text_div.find('.other_input_box_div').hide()
            }
        });
        $(document).ready(function () {
            $('.edit_anchor').click(function () {
                $(this).parents('.panel-view').find('#manual_expertise').toggle();
                $(this).parents('.panel-view').find('#update_expertise').toggle();
                $(this).parents('.panel-view').find('#manual_professional').toggle();
                $(this).parents('.panel-view').find('#update_professional').toggle();
                $(this).parents('.panel-view').find('#manual_achievement').toggle();
                $(this).parents('.panel-view').find('#update_achievement').toggle();
                $(this).parents('.panel-view').find('#manual_business').toggle();
                $(this).parents('.panel-view').find('#update_business').toggle();
                $(this).parents('.panel-view').find('#manual_stage').toggle();
                $(this).parents('.panel-view').find('#update_stage').toggle();


            })

            $('.save_anchor').click(function () {
                $(this).parents('.panel-view').find('#manual_expertise').toggle();
                $(this).parents('.panel-view').find('#update_expertise').toggle();
                $(this).parents('.panel-view').find('#manual_professional').toggle();
                $(this).parents('.panel-view').find('#update_professional').toggle();
                $(this).parents('.panel-view').find('#manual_achievement').toggle();
                $(this).parents('.panel-view').find('#update_achievement').toggle();
                $(this).parents('.panel-view').find('#manual_business').toggle();
                $(this).parents('.panel-view').find('#update_business').toggle();
                $(this).parents('.panel-view').find('#manual_stage').toggle();
                $(this).parents('.panel-view').find('#update_stage').toggle();

                var area_of_expertise = $("select[name='area_of_expertise[]']").map(function () { return $(this).val(); }).get();

                var stages = $('.stages:checked').map(function () {
                    return this.value;
                }).get();

                var advising_activities = $('.advising_activities:checked').map(function () {
                    return this.value;
                }).get();


                var primary_industry = $('#pi').val();
                var industry_roles = $(this).parents('.panel-view').find("select[name='industry_roles[]']").map(function () { return $(this).val(); }).get();
                var your_company = $(this).parents('.panel-view').find("input[name='your_company[]']").map(function () { return $(this).val(); }).get();
                var other_industry_roles = $(this).parents('.panel-view').find("input[name='other_industry_roles[]']").map(function () { return $(this).val(); }).get();
                var contribution = $(this).parents('.panel-view').find("textarea[name='contribution[]']").map(function () { return $(this).val(); }).get();

                var achievements = $('#ac').val();
                var business_problems = $('#bp').val();

                var area_of_business = $('#aob').val();
                var business_experience = $('#be').val();
                var ownership_experience = $('#oe').val();
                var countries_experience = $('#ce').val();


                if (area_of_expertise != 'undefined' || primary_industry != 'undefined' || industry_roles != 'undefined' || contribution != 'undefined' || achievements != 'undefined' || business_problems != 'undefined' || advising_activities != 'undefined' || area_of_business != 'undefined' || stages != 'undefined' || business_experience != 'undefined' || ownership_experience != 'undefined' || countries_experience != 'undefined') {
                    var post_data = { area_of_expertise: area_of_expertise, primary_industry: primary_industry, industry_roles: industry_roles, other_industry_roles: other_industry_roles, your_company: your_company, contribution: contribution, achievements: achievements, business_problems: business_problems, advising_activities: advising_activities, area_of_business: area_of_business, business_experience: business_experience, ownership_experience: ownership_experience, countries_experience: countries_experience, stages: stages }
                    $.ajax({
                        type: "POST",
                        url: base_url + 'Ajaxcontroller/ajax_update_fields',
                        data: post_data,
                        async: false,
                        dataType: "json",//return type expected as json
                        beforeSend: function () {
                            $('.loading_more').show();
                            $('body').addClass('loader-active');

                        },
                        success: function (response) {
                            $('.loading_more').fadeOut(500);
                            $('body').removeClass('loader-active');
                            if (response.status) {
                                if (!jQuery.isEmptyObject(response.mentor)) {
                                    if (response.mentor.area_of_expertise != '' || response.mentor.area_of_expertise != null)
                                        $('#manual_expertise').find('#area_of_expertise').html(response.mentor.area_of_expertise);

                                    if (response.mentor.primary_industry != '' || response.mentor.primary_industry != null)
                                        $('#manual_expertise').find('#primary_industry').html(response.mentor.primary_industry);

                                    if (response.mentor.industry_roles != '' || response.mentor.industry_roles != null)
                                        $('#manual_professional').find('#industry_roles').html(response.mentor.industry_roles);


                                    if (response.mentor.achievements != '' || response.mentor.achievements != null)
                                        $('#manual_achievement').find('#achievements').html(response.mentor.achievements);

                                    if (response.mentor.business_problems != '' || response.mentor.business_problems != null)
                                        $('#manual_business').find('#business_problems').html(response.mentor.business_problems);

                                    if (response.mentor.advising_activities != '' || response.mentor.advising_activities != null)
                                        $('#manual_business').find('#advising_activities').html(response.mentor.advising_activities);

                                    if (response.mentor.area_of_business != '' || response.mentor.area_of_business != null)
                                        $('#manual_business').find('#area_of_business').html(response.mentor.area_of_business);

                                    if (response.mentor.stages != '' || response.mentor.stages != null)
                                        $('#manual_stage').find('#stages').html(response.mentor.stages);

                                    if (response.mentor.business_experience != '' || response.mentor.business_experience != null)
                                        $('#manual_stage').find('#business_experience').html(response.mentor.business_experience);

                                    if (response.mentor.ownership_experience != '' || response.mentor.ownership_experience != null)
                                        $('#manual_stage').find('#ownership_experience').html(response.mentor.ownership_experience);

                                    if (response.mentor.countries_experience != '' || response.mentor.countries_experience != null)
                                        $('#manual_stage').find('#countries_experience').html(response.mentor.countries_experience);

                                }
                            }
                            else {
                                alert(response.msg);
                                return false;
                            }

                            $('.selectpicker').selectpicker('refresh');
                        }
                    });
                }

            })
            $("#aa_all").click(function () {

                $(".advising_activities").prop("checked", $(this).prop("checked"));
                //$(".advising_activities").prop("checked",false);
            });

            $(".advising_activities").click(function () {
                var sList = 0;
                $('.advising_activities').each(function () {
                    if ($(this).is(":checked")) {
                        sList = sList + 1;
                    }
                });
                if (sList < 5)
                    $('#aa_all').attr('checked', false);
                if (sList == 4) {
                    $("#aa_all").prop("checked", $(this).prop("checked"));
                }
            })
        })
</script>