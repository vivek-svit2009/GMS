                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="indicator"></div>
    </div>

</section>
<section class="section about-mentor bg-grey">
    <div class="container">
        <header class="header-title text-center">
            <h3 class="title">Meet Our Mentors</h3>
            <p>Over 100 mentors from various domains and expertise. Get personalized incubation support from our handpicked angel investors. The engagement would be a blend of emails, live chats, scheduled calls & face-to-face sessions.</p>
        </header>

        <div class="mentor-carousel owl-carousel">
            @foreach (var item in Model)
            {
            <div class="item">
                <div class="mentors">
                    <figure><a href="/mentor-profile/@item.Id"><img src="/assets/uploads/frontuser/@item.Image" alt=""></a></figure>

                    <div class="mid-body">
                        <div class="user-rating">
                            <div class="rating-count">

                                <img src="/assets/frontend/images/star-off.png" alt="">

                                <img src="/assets/frontend/images/star-off.png" alt="">

                                <img src="/assets/frontend/images/star-off.png" alt="">

                                <img src="/assets/frontend/images/star-off.png" alt="">

                                <img src="/assets/frontend/images/star-off.png" alt="">
                            </div>
                        </div>
                        <h4>@item.Name</h4>
                        <p><h5>@item.Designation</h5> </p>
                        <p>
                            @item.Address
                        </p>
                        <p class="invest-type"><small>Also an investor</small></p>
                    </div>
                    @{
                        string[] aoe = item.AreaOfInterest.Remove(item.AreaOfInterest.Length - 1, 1).Split(',');

                    }
                    <div class="chips-view text-sm">
                        @{
                            foreach (var a in aoe)
                            {
                                <span>@a</span>
                            }
                        }
                    </div>
                    <div class="p-cta">
                        <a href="/mentor-profile/@item.Id" class="btn-vw">View Details</a>
                        <a href="/user/connect/@item.Id" class="btn-cn">Connect</a>
                    </div>
                </div>
            </div>
            }

        </div>

        <div class="text-center load-more">
            <a href="/meet-our-mentors" class="btn btn-lg btn-default">View All Mentors</a>
            <a href="/meet-our-startups" class="btn btn-lg btn-default">View All Startups</a>
        </div>

    </div>
</section>

