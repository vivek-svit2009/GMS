<section class="section stay-in-touch">
    <div class="container">
        <header class="header-title text-center">
            <h3 class="title-lg">We would like to stay in touch</h3>
        </header>
        <form action="/ajaxcontroller/getInTuch" id="subscribe_form" name="pForm" autocomplete="off" method="post" accept-charset="utf-8">


            <div class="row stay-touch-form row-gap">
                <div class="col-md-3 col-sm-6 col-xs-12 form-group form-group-lg">
                    <input type="text" name="f_name" value="" class="form-control" id="f_name" placeholder="First Name" maxlength="80" />


                    <div id="ef_name" class="text-danger er"></div>

                </div>
                <div class="col-md-3 col-sm-6 col-xs-12 form-group form-group-lg">
                    <input type="text" name="l_name" value="" class="form-control" id="l_name" placeholder="Last Name" maxlength="80" />


                    <div id="el_name" class="text-danger er"></div>

                </div>
                <div class="col-md-4 col-xs-12 form-group form-group-lg">
                    <input type="text" name="s_email" value="" class="form-control" id="s_email" placeholder="Email Address" maxlength="80" />

                    <div id="es_email" class="text-danger er"></div>
                </div>
                <div class="col-md-2 col-md-offset-0 col-sm-4 col-sm-offset-4 col-xs-12 form-group">
                    <a href="javascript:void(0)" id="getintuch" class="btn btn-lg btn-primary btn-block">Submit</a>
                </div>

                <div class="col-xs-12 text-center">
                    <div id="msg" class="text-success"></div>
                </div>

            </div>
        </form>
    </div>
</section>
<script type="text/javascript">
    $(document).ready(function () {
        $("#getintuch").click(function (evt) {
            $(this).prop('disabled', true);
            $thisv = $(this);
            var first_name = $("#f_name").val();
            var last_name = $("#l_name").val();
            var email = $("#s_email").val();

            $.ajax({
                url: '/Ajaxcontroller/getInTuch',
                type: 'POST',
                data: { first_name: first_name, last_name: last_name, email: email },
                datatype: 'json',
                cache: false,
                success: function (data) {
                    var dataar = jQuery.parseJSON(data);

                    if (dataar.type == 'success') {
                        $('#ef_name').html('');
                        $('#el_name').html('');
                        $('#es_email').html('');
                        var msg = dataar.msg;
                        $('#msg').html(msg);
                        $("#subscribe_form")[0].reset();
                    }
                    else {

                        var first_name = dataar.first_name;
                        var last_name = dataar.last_name;
                        var email = dataar.email;

                        if (first_name)
                            $('#ef_name').html(first_name);
                        else
                            $('#ef_name').html('');
                        if (last_name)
                            $('#el_name').html(last_name);
                        else
                            $('#el_name').html('');
                        if (email)
                            $('#es_email').html(email);
                        else if (dataar.msg == 'allready')
                            $('#es_email').html("This email id already exists.");
                        else
                            $('#es_email').html('');
                        $('#msg').html('');
                    }
                    $thisv.prop('disabled', false);
                }
            });
        });
    });
</script>


